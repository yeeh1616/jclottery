function Qxc() {
	
	/**
	 * 计算注数
	 * @param betArray,二维数组
	 */
	this.calcBetNum = function(betArray) {
		var betNum = 1;
		for ( var i = 0; i < betArray.length; i++ ) {
			betNum *= betArray[i].length;
		}
		return betNum;
	}
}