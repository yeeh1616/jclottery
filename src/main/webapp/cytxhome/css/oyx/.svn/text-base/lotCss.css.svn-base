/*右侧开奖结果计算器图标*/
*html {
	background-image: url(about:blank);
	background-attachment: fixed;
}
.Gn_fix {
	width: 58px;
	height: 126px;
	position: fixed;
	top: 231px;
	left: 1472px;
}
