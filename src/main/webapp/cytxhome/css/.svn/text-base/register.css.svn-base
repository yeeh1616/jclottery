	padding: 2px 3px;
	line-height: 28px;
	border-radius: 3px;
}
.rec_register_w .active {
	border-color: #2db771!important;
}
.rec_btn {
	display: block;
	width: 100px;
	height: 28px;
	border-radius: 3px;
	background: #ccc;
	text-align: center;
	line-height: 28px;
	color: #fff;
	margin-left: 149px;
}
.zhuce_tab {
	width: 810px;
